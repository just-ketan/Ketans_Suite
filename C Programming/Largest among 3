#include <stdio.h>
#include <stdbool.h>

int main()
{
// find largest among 3 numbers
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    if(a>b && a>c){
        printf("a is largest");
    }else{
        if(b>c && b>a){
            printf("b is largest");
        }else{
            if(c>a && c>b){
                printf("c is largest");
            }
        }
    }
    return 0;
}
